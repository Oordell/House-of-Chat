{
  "expo": {
    "name": "HouseOfChat",
    "slug": "HouseOfChat",
    "version": "1.0.1",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
